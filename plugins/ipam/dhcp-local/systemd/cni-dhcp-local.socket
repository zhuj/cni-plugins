[Unit]
Description=CNI DHCP-LOCAL service socket
Documentation=https://github.com/containernetworking/plugins/tree/master/plugins/ipam/dhcp
PartOf=cni-dhcp-local.service

[Socket]
ListenStream=/run/cni/dhcp-local.sock
SocketMode=0660
SocketUser=root
SocketGroup=root
RemoveOnStop=true

[Install]
WantedBy=sockets.target
